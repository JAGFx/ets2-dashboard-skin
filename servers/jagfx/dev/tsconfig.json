{
  "compilerOptions": {
    "paths" : {
      "@/*": ["./src/*"],
      "@core/*": ["../../../lib/jagfx/core/src/*"],
      "@server/*": ["../../../lib/jagfx/server/src/*"]
    },
    "baseUrl" : ".",
    "target": "ESNext",
    "module": "ESNext",
    "outDir": "dist",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "strict": true,
	"moduleResolution": "node",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strictPropertyInitialization": false
  }
}