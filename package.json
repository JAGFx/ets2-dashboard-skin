{
  "name": "ets2-dashboard-skin",
  "version": "1.8.0",
  "private": false,
  "module": "true",
  "description": "Euro Truck Simulator 2 dashboard",
  "author": {
    "name": "Emmanuel SMITH",
    "email": "hey@emmanuel-smith.me",
    "url": "https://emmanuel-smith.me"
  },
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "npx eslint src lib servers test --ext .js,.vue --fix",
    "lint:prettier": "npx prettier --config .prettierrc src/**/*.{js,vue} lib/**/*.js servers/**/*.js test/**/*.js  --write",
    "test": "npm run test:unit",
    "test:unit": "npx jest",
    "test:coverage": "npx jest --coverage",
    "test:coverage:preview": "npm run test:coverage && npx http-server coverage/lcov-report/ -p 8002",
    "build": "set NODE_ENV=production && npx gulp build",
    "bundle": "set NODE_ENV=production && npx gulp bundle",
    "bAndB": "npx gulp bAndB",
    "start": "npm run server:start",
    "dashboard:dev": "vue-cli-service serve --modern",
    "dashboard:build": "vue-cli-service build",
    "dashboard:start": "http-server dist/",
    "dashboard:i18n-extract": "vue-i18n-extract report --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/translations/*.?(yml|yaml)' --add",
    "server:install": "cd servers/default && npm i && npm rb",
    "server:build": "cd servers/default && set NODE_ENV=production && npm run build",
    "server:start": "cd servers/default && npm start",
    "server:dev": "cd servers/dev && npm start",
    "tiles:serv": "http-server ./maps -p 8081 --cors -s",
    "font:build": "npx gulp build:font",
    "commitSince": "git log --pretty=\"%h - %s (%an)\" v1.4.0-beta.0..HEAD"
  },
  "main": "index.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "core-js": "^3.23.3",
    "country-list": "^2.2.0",
    "emoji-flag": "^1.1.0",
    "emoji-flags": "~1.3.0",
    "ets2-dashboard-lib": "file:lib",
    "file-saver": "^2.0.5",
    "gsap": "^3.10.4",
    "json-query": "^2.2.2",
    "lodash": "^4.17.21",
    "luxon": "^2.4.0",
    "ol": "^6.14.1",
    "socket.io-client": "^4.5.1",
    "units-converter": "^1.0.3",
    "vue": "^2.7.3",
    "vue-clipboard2": "^0.3.3",
    "vue-insomnia": "^0.1.0",
    "vue-progressbar": "^0.7.5",
    "vuex": "^3.6.2",
    "yaml-loader": "0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/vue2-jest": "^27.0.0",
    "babel-jest": "^27.5.1",
    "bourbon": "~7.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-html": "^6.2.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "fs-extra": "^10.1.0",
    "gulp": "~4.0.2",
    "gulp-gzip": "~1.4.2",
    "gulp-load-plugins": "~2.0.5",
    "gulp-run-command": "0.0.10",
    "gulp-tar": "~3.1.0",
    "http-server": "^13.1.0",
    "icon-font-generator": "^2.1.11",
    "jest": "^27.5.1",
    "jest-transform-yaml": "^1.0.0",
    "node-sass": "^6.0.1",
    "pkg": "^5.7.0",
    "prettier": "^2.7.1",
    "sass-loader": "^10.3.1",
    "vue-i18n-extract": "^2.0.7",
    "vue-template-compiler": "^2.7.3"
  },
  "bugs": {
    "url": "https://github.com/JAGFx/ets2-dashboard-skin/issues"
  },
  "homepage": "https://jagfx.github.io/ets2-dashboard-skin/",
  "keywords": [
    "ets2",
    "eurotrucksimulator2",
    "dashboard",
    "telemetry"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JAGFx/ets2-dashboard-skin.git"
  }
}
