{
  "name": "ets2-dashboard-skin",
  "version": "1.7.2",
  "private": false,
  "type": "module",
  "description": "Euro Truck Simulator 2 dashboard",
  "author": {
    "name": "Emmanuel SMITH",
    "email": "hey@emmanuel-smith.me",
    "url": "https://emmanuel-smith.me"
  },
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:eslint": "npx eslint src/jagfx --fix -c tools/.eslintrc.json",
    "lint:prettier": "npx prettier {src/jagfx,lib/jagfx,servers/jagfx}/**/*.{js,vue,ts} !dist/**/* --write --config tools/.prettierrc.json",
    "lint:stylelint": "npx stylelint --fix src/jagfx/**/*.scss --config tools/.stylelintrc.json",
    "test": "npm run test:unit",
    "test:unit": "npx jest src/jagfx -c tools/.jestrc.ts --passWithNoTests",
    "test:coverage": "npx jest --coverage -c tools/.jestrc.ts",
    "test:coverage:preview": "npm run test:coverage && npx http-server coverage/lcov-report/ -p 8002",
    "ci": "npm run lint && npm run test",
    "build": "set NODE_ENV=production && npx gulp -f tools/.gulpfile.js build",
    "bundle": "set NODE_ENV=production && npx gulp -f tools/.gulpfile.js bundle",
    "bAndB": "npx gulp -f tools/.gulpfile.js bAndB",
    "start": "npm run server:start",
    "dashboard:dev": "npx vite -c tools/.viterc.ts --host --port 8080 --strictPort",
    "dashboard:build": "npx vue-tsc -p tools/tsconfig.json --noEmit && npx vite build -c tools/.viterc.ts",
    "dashboard:start": "http-server dist/",
    "dashboard:i18n-extract": "vue-i18n-extract report --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/translations/*.?(yml|yaml)' --add",
    "server:install": "cd servers/default && npm i && npm rb",
    "server:build": "cd servers/default && set NODE_ENV=production && npm run build",
    "server:start": "cd servers/default && npm start",
    "server:dev": "cd servers/dev && npm start",
    "tiles:serv": "http-server ./maps -p 8081 --cors -s",
    "font:build": "npx gulp -f tools/.gulpfile.js build:font",
    "commitSince": "git log --pretty=\"%h - %s (%an)\" v1.4.0-beta.0..HEAD"
  },
  "main": "index.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.4.0",
    "@fortawesome/fontawesome-svg-core": "^6.4.0",
    "@fortawesome/free-brands-svg-icons": "^6.4.0",
    "@fortawesome/free-solid-svg-icons": "^6.4.0",
    "@fortawesome/vue-fontawesome": "^3.0.3",
    "@vueuse/core": "^9.13.0",
    "axios": "^0.27.2",
    "country-list": "^2.3.0",
    "emoji-flag": "^1.1.0",
    "emoji-flags": "~1.3.0",
    "file-saver": "^2.0.5",
    "gsap": "^3.12.2",
    "json-query": "^2.2.2",
    "lodash": "^4.17.21",
    "luxon": "^2.5.2",
    "ol": "^6.15.1",
    "socket.io-client": "^4.7.1",
    "units-converter": "^1.0.3",
    "vue": "^3.2.45",
    "vue3-touch-events": "^4.1.3"
  },
  "devDependencies": {
    "@modyfi/vite-plugin-yaml": "^1.0.4",
    "@trivago/prettier-plugin-sort-imports": "^3.4.0",
    "@types/jest": "^29.5.3",
    "@types/jest-json-schema": "^6.1.1",
    "@types/luxon": "^3.3.1",
    "@types/node": "^18.17.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/compiler-sfc": "^3.2.39",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/vue3-jest": "^29.2.4",
    "bootstrap": "~5.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.15.1",
    "fs-extra": "^10.1.0",
    "gulp": "~4.0.2",
    "gulp-gzip": "~1.4.2",
    "gulp-load-plugins": "~2.0.5",
    "gulp-run-command": "0.0.10",
    "gulp-tar": "~3.1.0",
    "http-server": "^13.1.0",
    "icon-font-generator": "^2.1.11",
    "jest": "^29.6.1",
    "jest-json-schema": "^6.1.0",
    "pkg": "^5.8.1",
    "postcss-plugin": "^1.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.64.1",
    "stylelint": "^14.16.1",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.4.6",
    "vue-eslint-parser": "^9.3.1",
    "vue-i18n-extract": "^2.0.7",
    "vue-tsc": "^0.40.13",
    "yaml-jest": "github:JAGFx/yaml-jest"
  },
  "bugs": {
    "url": "https://github.com/JAGFx/ets2-dashboard-skin/issues"
  },
  "homepage": "https://jagfx.github.io/ets2-dashboard-skin/",
  "keywords": [
    "ets2",
    "eurotrucksimulator2",
    "dashboard",
    "telemetry"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JAGFx/ets2-dashboard-skin.git"
  },
  "workspaces": [
    "lib/jagfx/core",
    "lib/jagfx/server",
    "servers/jagfx/dev"
  ]
}
