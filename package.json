{
  "name": "ets2-dashboard-skin",
  "version": "1.7.2",
  "private": false,
  "description": "Euro Truck Simulator 2 dashboard",
  "author": {
    "name": "Emmanuel SMITH",
    "email": "hey@emmanuel-smith.me",
    "url": "https://emmanuel-smith.me"
  },
  "scripts": {
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:stylelint",
    "lint:eslint": "npx eslint {src/jagfx,lib,servers}/**/*.{js,vue} --fix -c tools/.eslintrc.json",
    "lint:prettier": "npx prettier {src/jagfx,src/domain,lib,servers}/**/*.{js,vue} --write --config tools/.prettierrc.json",
    "lint:stylelint": "npx stylelint --fix src/jagfx/**/*.scss --config tools/.stylelintrc.json",
    "test": "npm run test:unit",
    "test:unit": "npx jest -c tools/.jestrc.js",
    "test:coverage": "npx jest --coverage -c tools/.jestrc.js",
    "test:coverage:preview": "npm run test:coverage && npx http-server coverage/lcov-report/ -p 8002",
    "ci": "npm run lint && npm run test",
    "build": "set NODE_ENV=production && npx gulp -f tools/.gulpfile.js build",
    "bundle": "set NODE_ENV=production && npx gulp -f tools/.gulpfile.js bundle",
    "bAndB": "npx gulp -f tools/.gulpfile.js bAndB",
    "start": "npm run server:start",
    "dashboard:dev": "npx vite -c tools/.vite.mjs --host --port 8080 --strictPort",
    "dashboard:build": "npx vite -c tools/.vite.mjs build",
    "dashboard:start": "http-server dist/",
    "dashboard:i18n-extract": "vue-i18n-extract report --vueFiles './src/**/*.?(js|vue)' --languageFiles './src/translations/*.?(yml|yaml)' --add",
    "server:install": "cd servers/default && npm i && npm rb",
    "server:build": "cd servers/default && set NODE_ENV=production && npm run build",
    "server:start": "cd servers/default && npm start",
    "server:dev": "cd servers/dev && npm start",
    "tiles:serv": "http-server ./maps -p 8081 --cors -s",
    "font:build": "npx gulp -f tools/.gulpfile.js build:font",
    "commitSince": "git log --pretty=\"%h - %s (%an)\" v1.4.0-beta.0..HEAD"
  },
  "main": "index.js",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-brands-svg-icons": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/vue-fontawesome": "^3.0.1",
    "@vue/compat": "^3.2.37",
    "axios": "^0.27.2",
    "core-js": "^3.24.1",
    "country-list": "^2.2.0",
    "emoji-flag": "^1.1.0",
    "emoji-flags": "~1.3.0",
    "ets2-dashboard-lib": "file:lib",
    "file-saver": "^2.0.5",
    "gsap": "^3.10.4",
    "json-query": "^2.2.2",
    "lodash": "^4.17.21",
    "luxon": "^2.5.0",
    "ol": "^6.15.1",
    "pinia": "^2.0.18",
    "socket.io-client": "^4.5.1",
    "units-converter": "^1.0.3",
    "vue": "^3.2.37",
    "vue-clipboard2": "^0.3.3",
    "vue-insomnia": "github:JAGFx/vue-insomnia#master",
    "vue-progressbar": "^0.7.5",
    "vue3-touch-events": "^4.1.0",
    "vuex": "^4.0.2",
    "yaml-loader": "0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@jest/globals": "^28.1.3",
    "@modyfi/vite-plugin-yaml": "^1.0.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/jest": "^28.1.6",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.2.37",
    "@vue/vue3-jest": "^28.0.1",
    "babel-jest": "^28.1.3",
    "bootstrap": "~5.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "fs-extra": "^10.1.0",
    "gulp": "~4.0.2",
    "gulp-gzip": "~1.4.2",
    "gulp-load-plugins": "~2.0.5",
    "gulp-run-command": "0.0.10",
    "gulp-tar": "~3.1.0",
    "http-server": "^13.1.0",
    "icon-font-generator": "^2.1.11",
    "jest": "^28.1.3",
    "pkg": "^5.8.0",
    "postcss-plugin": "^1.0.0",
    "prettier": "^2.7.1",
    "sass": "^1.54.4",
    "stylelint": "^14.10.0",
    "stylelint-config-prettier-scss": "^0.0.1",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard-scss": "^5.0.0",
    "vite": "^2.9.14",
    "vue-i18n-extract": "^2.0.7",
    "yaml-jest": "github:hvasc/yaml-jest#support-jest28"
  },
  "bugs": {
    "url": "https://github.com/JAGFx/ets2-dashboard-skin/issues"
  },
  "homepage": "https://jagfx.github.io/ets2-dashboard-skin/",
  "keywords": [
    "ets2",
    "eurotrucksimulator2",
    "dashboard",
    "telemetry"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/JAGFx/ets2-dashboard-skin.git"
  }
}
